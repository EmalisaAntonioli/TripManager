@model List<Location>

<h1>My Locations</h1>

<table>
    <tr>
        <th>@Html.DisplayNameFor(model => model[0].Name)</th>
        <th>@Html.DisplayNameFor(model => model[0].Description)</th>
    </tr>
    @foreach (var location in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => location.Name)</td>
            <td>@Html.DisplayFor(modelItem => location.Description)</td>
        </tr>
    }
</table>
